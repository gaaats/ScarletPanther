<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nnnnnnnnnaaaavvff"
    app:startDestination="@id/gamq1Fragment">

    <fragment
        android:id="@+id/gamq1Fragment"
        android:name="com.superking.parchisi.stare.frrraag.Gamq1Fragment"
        android:label="fragment_gamq1"
        tools:layout="@layout/fragment_gamq1" >
        <action
            android:id="@+id/action_gamq1Fragment_to_game2Fragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out"
            app:destination="@id/game2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/game2Fragment"
        android:name="com.superking.parchisi.stare.frrraag.Game2Fragment"
        android:label="fragment_game2"
        tools:layout="@layout/fragment_game2" >
        <action
            android:id="@+id/action_game2Fragment_to_gamq1Fragment"
            app:destination="@id/gamq1Fragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/gamq1Fragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_game2Fragment_to_game3Fragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out"
            app:destination="@id/game3Fragment" />
    </fragment>
    <fragment
        android:id="@+id/game3Fragment"
        android:name="com.superking.parchisi.stare.frrraag.Game3Fragment"
        android:label="fragment_game3"
        tools:layout="@layout/fragment_game3" >
        <action
            android:id="@+id/action_game3Fragment_to_game2Fragment"
            app:destination="@id/game2Fragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/game2Fragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>