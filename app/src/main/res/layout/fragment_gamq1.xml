<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/bbback"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".oootththht.Gamq1Fragment">



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gojgtjtgjitgjijtgi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/jthjgtjtgijitgjjgt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />


    <ImageView
        android:id="@+id/img_titple_game111"
        android:layout_width="220dp"
        android:layout_height="220dp"
        android:scaleType="centerInside"
        android:src="@drawable/logg"
        app:layout_constraintBottom_toTopOf="@id/btn_play_game"
        app:layout_constraintEnd_toEndOf="@id/gojgtjtgjitgjijtgi"
        app:layout_constraintStart_toStartOf="@id/gtgtjgtigtgtjgtj"
        app:layout_constraintTop_toTopOf="@id/jthjgtjtgijitgjjgt" />

    <ImageView
        android:id="@+id/img_titple_game222"
        android:layout_width="220dp"
        android:layout_height="220dp"
        android:scaleType="centerInside"
        android:src="@drawable/plaaane"
        app:layout_constraintBottom_toBottomOf="@id/gtjitgjgtjjgtjgtj"
        app:layout_constraintTop_toBottomOf="@id/btn_play_game"
        app:layout_constraintEnd_toEndOf="@id/gojgtjtgjitgjijtgi"
        app:layout_constraintStart_toStartOf="@id/gtgtjgtigtgtjgtj"
         />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gtjitgjgtjjgtjgtj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gijtjtgjjgtjgtj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.50" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gtgtjgtigtgtjgtj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />



    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_play_game"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_black"
        android:text="launch game"
        android:layout_marginTop="32dp"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@id/gojgtjtgjitgjijtgi"
        app:layout_constraintStart_toStartOf="@id/gtgtjgtigtgtjgtj"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_chainStyle="packed" />




</androidx.constraintlayout.widget.ConstraintLayout>